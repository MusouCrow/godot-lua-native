[configuration]

entry_symbol = "luagd_library_init"
compatibility_minimum = "4.1"
reloadable = true

[libraries]

macos.debug.arm64 = "libluagd.macos.template_debug.arm64.dylib"
macos.release.arm64 = "libluagd.macos.template_release.arm64.dylib"
macos.debug.x86_64 = "libluagd.macos.template_debug.x86_64.dylib"
macos.release.x86_64 = "libluagd.macos.template_release.x86_64.dylib"
windows.debug.x86_64 = "libluagd.windows.template_debug.x86_64.dll"
windows.release.x86_64 = "libluagd.windows.template_release.x86_64.dll"
linux.debug.x86_64 = "libluagd.linux.template_debug.x86_64.so"
linux.release.x86_64 = "libluagd.linux.template_release.x86_64.so"
linux.debug.arm64 = "libluagd.linux.template_debug.arm64.so"
linux.release.arm64 = "libluagd.linux.template_release.arm64.so"
